{
    if distance_to_object(driver) &gt; 200
    {
        if driver.x &gt; x
            state = "standingRight"
        else
            state = "standingLeft"
    }
}

vspeed += myGravity

if (!place_free(x, y+vspeed))
and state == "jumpingLeft"
{
    state = "runningLeft"
}
if (!place_free(x, y+vspeed))
and state == "jumpingRight"
{
    state = "runningRight"
}
